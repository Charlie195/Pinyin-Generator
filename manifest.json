{
    "name": "Pinyin Generator",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://mp.weixin.qq.com/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": [
            "character-source.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}