{
    "name": "Pinyin Generator",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://mp.weixin.qq.com/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": [
            "character-source.js",
            "background.js"
        ]
    }
}