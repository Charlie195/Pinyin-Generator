{
    "name": "Pinyin Generator",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["extensionWindow.css"],
            "js": ["content.js", "character-source.js"]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": ["revert.png"],
          "matches": ["<all_urls>"]
        }
    ]
}